  // éªŒè¯åœ°å€æ˜¯å¦æœ‰æ•ˆï¼ˆæ”¯æŒå¤šç§ç½‘ç»œï¼‰
  isValidAddress(address) {
    if (!address || typeof address !== 'string') return false;
    
    // TRONåœ°å€ï¼?4ä¸ªå­—ç¬¦çš„base58å­—ç¬¦ä¸²ï¼Œä»¥Tå¼€å¤?    if (address.length === 34 && address.startsWith('T')) {
