    }
    
    // TRON hexåœ°å€ï¼?2ä¸ªå­—ç¬¦ï¼Œä»?1å¼€å¤?    if (address.length === 42 && address.startsWith('41')) {
      return true;
    }
    
    // BSC/ETHåœ°å€ï¼?2ä¸ªå­—ç¬¦ï¼Œä»?xå¼€å¤?    if (address.length === 42 && address.startsWith('0x')) {
      return true;
    }
    
    return false;
  }

  // æå–æœ‰æ•ˆçš„åœ°å€å­—ç¬¦ä¸?  extractAddress(addressInput) {
    if (!addressInput) return null;
    
    // å¦‚æœæ˜¯å­—ç¬¦ä¸²ï¼ŒéªŒè¯åè¿”å›
    if (typeof addressInput === 'string') {
      const trimmed = addressInput.trim();
      // æ£€æŸ¥æ˜¯å¦æ˜¯æœ‰æ•ˆçš„TRONåœ°å€
      if (this.isValidAddress(trimmed)) {
        return trimmed;
      }
      return null;
    }
    
    // å¦‚æœæ˜¯å¯¹è±¡ï¼Œå°è¯•æå–åœ°å€
    if (typeof addressInput === 'object') {
      // å°è¯•ä»tronWebåœ°å€å¯¹è±¡ä¸­æå?      if (addressInput.base58 && typeof addressInput.base58 === 'string') {
        const addr = addressInput.base58.trim();
        if (this.isValidAddress(addr)) {
          return addr;
        }
      }
      if (addressInput.hex && typeof addressInput.hex === 'string') {
        const addr = addressInput.hex.trim();
        if (this.isValidAddress(addr)) {
          return addr;
        }
      }
      
      // æ£€æŸ¥æ˜¯å¦æ˜¯Vueçš„reactiveå¯¹è±¡
      if (addressInput.value && typeof addressInput.value === 'string') {
        const addr = addressInput.value.trim();
        if (this.isValidAddress(addr)) {
          return addr;
        }
      }
      
      // å¦‚æœæœ‰toStringæ–¹æ³•ä¸”ä¸æ˜¯é»˜è®¤çš„Object toString
      if (addressInput.toString && typeof addressInput.toString === 'function') {
        try {
          const str = addressInput.toString();
          if (str && str !== '[object Object]' && str.length > 10) {
            const addr = str.trim();
            if (this.isValidAddress(addr)) {
              return addr;
            }
          }
        } catch (e) {
          console.warn('toStringæ–¹æ³•è°ƒç”¨å¤±è´¥:', e);
        }
      }
      
      // å°è¯•ç›´æ¥ä»å¯¹è±¡å±æ€§ä¸­æŸ¥æ‰¾å¯èƒ½çš„åœ°å€
      const possibleAddressKeys = ['address', 'addr', 'account', 'publicKey'];
      for (const key of possibleAddressKeys) {
        if (addressInput[key] && typeof addressInput[key] === 'string') {
          const addr = addressInput[key].trim();
          if (this.isValidAddress(addr)) {
            return addr;
          }
        }
      }
    }
    
    console.warn('æ— æ³•ä»è¾“å…¥ä¸­æå–æœ‰æ•ˆåœ°å€:', addressInput);
    return null;
  }

